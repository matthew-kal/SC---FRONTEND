{
  "expo": {
    "name": "SurgiCalm",
    "slug": "SC---Frontend",
    "owner": "matthewkal",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "usesApplePushNotifications": true,
      "bundleIdentifier": "com.surgicalm.platform",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            },
            "192.168.1.72": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.surgicalm.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-audio",
      "expo-video", 
      "expo-notifications"
    ],
    "eas": {
      "projectId": "eb94781a-5040-4c76-8e2b-40ba048cbbdf"
    },
    "extra": {
      "eas": {
        "projectId": "7ceee751-70a4-4c90-a14e-d3980d118576"
      }
    }
  }
}
